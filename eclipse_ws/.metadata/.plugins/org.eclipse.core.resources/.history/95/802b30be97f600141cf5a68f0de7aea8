class Solution	{
	//Given an array of numbers, return an integer.
	public int getSpyNumber(int[] patchNumbers) {
	    //Please implement this
	    int n = patchNumbers.length;
	    
	    int target = (n+2)*(n+1)/2;
	    
	    int sum = 0;
	    for (int i = 0; i < n; i++)
	        sum += patchNumbers[i];
	   	
	    return 0;
	}
	
}

public class WhoWentAWOL {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Solution sol = new Solution()
		int []patchNumbers = {1,2,4};
		System.out.println(sol.getSpyNumber(patchNumbers));
	}

}
